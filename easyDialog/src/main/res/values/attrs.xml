<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CVDialogButton">
        <attr name="ad_text" format="string" />
        <attr name="ad_colorMain" format="reference|color" />
        <attr name="ad_colorSecond" format="reference|color" />
        <attr name="ad_cornerRadius" format="integer" />
    </declare-styleable>

    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="minWidth" format="dimension" />
        <attr name="maxWidth" format="dimension"/>
        <attr name="minHeight" format="dimension" />
        <attr name="maxHeight" format="dimension"/>
        <attr name="indicatorName" format="string"/>
        <attr name="indicatorColor" format="color"/>
    </declare-styleable>
</resources>